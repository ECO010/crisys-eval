<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.securityevaluationtool.EvaluationEndController">
   <children>
      <Label fx:id="evaluationEndHeading" layoutX="15.0" layoutY="27.0" prefHeight="20.0" prefWidth="566.0" text="This is the end of the evaluation for: $$SystemName$$">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button layoutX="358.0" layoutY="208.0" mnemonicParsing="false" onAction="#onReturnClick" text="Return To Main Menu" />
      <ProgressIndicator fx:id="systemSafetyScoreIndicator" layoutX="30.0" layoutY="71.0" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="248.0" prefWidth="262.0" progress="0.0" />
      <Button layoutX="358.0" layoutY="134.0" mnemonicParsing="false" onAction="#onDownloadClick" text="Download Evaluation Results" />
      <Hyperlink layoutX="50.0" layoutY="339.0" onAction="#onHyperlinkClick" text="Learn How This Score Was Calculated">
         <font>
            <Font size="13.0" />
         </font>
      </Hyperlink>
   </children>
</AnchorPane>
