<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.securityevaluationtool.PreparednessWindowController">
   <children>
      <Label layoutX="84.0" layoutY="322.0" text="Can't recall Mitigations? Click HERE to go back to the tree view and view mitgations">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font>
      </Label>
      <Label fx:id="assetInfoLabel" layoutX="33.0" layoutY="47.0" prefHeight="33.0" prefWidth="594.0" text="Considering the $$NumOfMitigations$$ potential mitigations suggested for protecting $$Asset Name$$,">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <ComboBox fx:id="comboBox" layoutX="33.0" layoutY="217.0" onAction="#onComboBoxClick" prefHeight="25.0" prefWidth="579.0" />
      <Button fx:id="continueBtn" layoutX="463.0" layoutY="352.0" mnemonicParsing="false" onAction="#onContinueClick" prefHeight="25.0" prefWidth="93.0" text="Continue" />
      <Button fx:id="backToTreeViewBtn" layoutX="259.0" layoutY="352.0" mnemonicParsing="false" onAction="#onBackClick" text="Back To Tree View" />
      <Label layoutX="33.0" layoutY="80.0" prefHeight="33.0" prefWidth="229.0" text="How secure do you think this asset is?">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label layoutX="33.0" layoutY="121.0" prefHeight="25.0" prefWidth="229.0" text="When selecting an option consider:">
         <font>
            <Font name="System Bold" size="11.0" />
         </font>
      </Label>
      <Button fx:id="previousAssetBtn" layoutX="84.0" layoutY="352.0" mnemonicParsing="false" text="Previous Asset" />
      <Label layoutX="33.0" layoutY="153.0" prefHeight="25.0" prefWidth="579.0" text="How many mitigations you are familiar with and How many mitigations you can confirm have been applied">
         <font>
            <Font name="System Bold" size="11.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
