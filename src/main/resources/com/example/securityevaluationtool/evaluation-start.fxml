<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="240.0" prefWidth="222.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.securityevaluationtool.EvaluationStartController">
   <children>
      <Label fx:id="labelTitle" layoutX="53.0" layoutY="58.0" prefHeight="34.0" prefWidth="292.0" text="Enter the name of the system to be evaluated">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button fx:id="backBtn" layoutY="256.0" mnemonicParsing="false" onAction="#onBackClick" text="Back to Main Menu" />
      <Button fx:id="continueBtn" layoutX="333.0" layoutY="255.0" mnemonicParsing="false" onAction="#onContinueClick" text="Continue" />
      <TextField fx:id="systemNameField" layoutY="147.0" prefHeight="25.0" prefWidth="398.0" />
   </children>
</AnchorPane>
