<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tooltip?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.securityevaluationtool.PreparednessWindowController">
   <children>
      <Label layoutX="108.0" layoutY="250.0" text="Can't recall Mitigations? Go back to the tree view to view mitgations">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font>
      </Label>
      <Label layoutX="19.0" layoutY="55.0" prefHeight="33.0" prefWidth="562.0" text="Based on the mitigations shown throughout the tree, how prepared are you to prevent this attack?">
         <tooltip>
            <Tooltip text="Mitigations shown throughout the tree includes mitigations for the root and all descendants" />
         </tooltip>
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <ComboBox fx:id="comboBox" layoutX="11.0" layoutY="142.0" onAction="#onComboBoxClick" prefHeight="25.0" prefWidth="579.0" />
      <Button fx:id="continueBtn" layoutX="428.0" layoutY="327.0" mnemonicParsing="false" onAction="#onContinueClick" text="Continue" />
      <Button fx:id="backToTreeViewBtn" layoutX="108.0" layoutY="327.0" mnemonicParsing="false" onAction="#onBackClick" text="Back To Tree View" />
   </children>
</AnchorPane>
